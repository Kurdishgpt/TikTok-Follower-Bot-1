entrypoint = "main.py"
modules = ["nodejs-20", "python-3.11"]

hidden = [".pythonlibs"]

[nix]
channel = "stable-23_05"
packages = ["geckodriver"]

[env]
LANG = "en_US.UTF-8"

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "run"

[[workflows.workflow]]
name = "run"
author = "agent"

[workflows.workflow.metadata]
outputType = "vnc"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
